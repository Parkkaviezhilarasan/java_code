class Solution {
    public int[] createTargetArray(int[] nums, int[] index) {
        int[] tar=new int[nums.length];
        for(int i=0;i<nums.length;i++){
            int pos=index[i];
            for(int j=nums.length-1;j>pos;j--){
                tar[j]=tar[j-1];
            }
            tar[pos]=nums[i];
        }
        return tar;
    }
}
